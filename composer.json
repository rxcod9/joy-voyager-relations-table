{
    "name": "joy/voyager-relations-table",
    "description": "This Laravel/Voyager module adds VoyagerRelationsTable support to Voyager.",
    "keywords": [
        "joy",
        "voyager-relations-table",
        "joy-voyager-relations-table",
        "voyager-voyager-relations-table",
        "cms",
        "voyager",
        "laravel"
    ],
    "homepage": "https://github.com/rxcod9/joy-voyager-relations-table",
    "license": "MIT",
    "authors": [{
        "name": "Ramakant Gangwar",
        "email": "gangwar.ramakant@gmail.com",
        "homepage": "https://github.com/rxcod9",
        "role": "Tech Lead & Architect"
    }],
    "require": {
        "php": "^7.3|^8.0|^9.0",
        "illuminate/support": "^7|^8|^9|^10",
        "joy/voyager-core": "^1.0|^2.0|^3.0",
        "joy/voyager-datatable": "^1.0|^2.0|^3.0|^4.0",
        "tcg/voyager": "^1.4|dev-1.6-l10"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "ext-zip": "*",
        "friendsofphp/php-cs-fixer": "^3.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "9.5.2",
        "phpunit/phpunit": "^9.1|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Joy\\VoyagerRelationsTable\\": "src"
        },
        "files": [
            "src/Http/helpers.php",
            "src/helper.php"
        ]
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Joy\\VoyagerRelationsTable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joy\\VoyagerRelationsTable\\VoyagerRelationsTableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "henzeb/composer-link": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
